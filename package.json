    "csv-parse": "^6.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
